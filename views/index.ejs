   <!-- add ejs code here, for each loop for each li item -->
 <%  for (let i = 0; i < imageArray.length; i++) {%>
    
    <li class="list-group-item"> <a class="port-spy" href="#item-<%=i%>"><%=imageArray[i].name%></a></li>
<%}%>
      
       <!-- end ejs here  -->
