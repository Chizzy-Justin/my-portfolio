                <div class="row">
                
                 
     <!-- add other ejs code here, same for each loop: change id number(item-1 and myModal-1), change image, based on arrays-->
    <% for (let i = 0; i < imageArray.length; i++) {%>
      
      
  
        <div class="col-lg-4 col-md-6 col-sm-12 port-display" id="item-<%=i%>">
                    <!-- Trigger the modal with a button -->
  
                   <a href="#portfolio" data-toggle="modal" data-target="#myModal-<%=i%>">
                   <article class="item">
                     <img src="<%=imageArray[i].images[0]%>" class="carousel-images" alt="work background" srcset="">
<!-- change name of this h2(the JAVA2) -->
                     <h2 id=""><%=imageArray[i].name%></h2>
  
                   </article>
                 </a>
                 </div>
                <%}%>
     <!-- end ejs here  -->
                        </div>
                      
                      </div>
                    </div>
                  </div>
                  
                 
        <!-- Modal -->
  <!-- add third ejs here, based on the second one: change id of myModal-1 for each, change all myCarousel-1 and images for the carousel from the image object array  -->
 <% for (let i = 0; i < imageArray.length; i++) {%>
    
    

  <div class="modal fade" id="myModal-<%=i%>" role="dialog">
      <div class="modal-dialog">
      
        <!-- Modal content-->
        <div class="modal-content">
         
          <div class="modal-body">
            
            <div id="myCarousel-<%=i%>" class="carousel slide" data-bs-ride="carousel">
              <button type="button" class="close" data-dismiss="modal">&times;</button>
              <div class="carousel-indicators">
                
                <button type="button" data-bs-target="#myCarousel-<%=i%>" data-bs-slide-to="0" class="c-ind active" aria-current="true" aria-label="Slide 1"></button>
                
                <%for (let j = 1; j < imageArray[i].images.length; j++) {%>
                  
                  <button type="button" data-bs-target="#myCarousel-<%=i%>" data-bs-slide-to="<%=j%>" class="c-ind" aria-label="Slide <%=j+1%>"></button>
              
              <%}%>
                 </div>
              <div class="carousel-inner">
                <div class="carousel-item active">
                   <img src="<%=imageArray[i].images[0]%>" class="carousel-image" alt="work background" srcset="">
                </div>
               <% for (let j = 1; j < imageArray[i].images.length; j++) {%>
                 
                  <div class="carousel-item">
                    <img src="<%=imageArray[i].images[j]%>" class="carousel-image" alt="work background" srcset="">
                  </div>
             <% }%>
               
                
              </div>
              <button class="carousel-control-prev" type="button" data-bs-target="#myCarousel-<%=i%>" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
              </button>
              <button class="carousel-control-next" type="button" data-bs-target="#myCarousel-<%=i%>" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
              </button>
            </div>
          </div>
         
        </div>
        
      </div>
    </div>
  <%}%>
    <!-- end third ejs here  -->
            
            </div>
            
            
           
        </div>
